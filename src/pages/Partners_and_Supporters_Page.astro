---
import '../styles/global.css';
import Navigation from '../components/Navigation.astro';
import { PartnersAndSupportersPage as PartnersContent } from '../components/partners-and-supporters-sections';
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>Partners & Supporters - SynBio Reactor Summit 2026</title>
		<meta name="description" content="Meet our partners, sponsors, and supporters powering the SynBio Reactor Summit ecosystem" />
	</head>

	<body class="min-h-screen bg-background">
		<Navigation />
		
		<main class="pt-16">
			<PartnersContent client:load />
		</main>
	</body>
</html>

