---
import '../styles/global.css';
import SBRFavicon from '@/assets/SBR2026-Favicon-1-60x60.png';
const { content } = Astro.props;
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/png" href={SBRFavicon.src} />
		<title>{content.title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
